<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/wqj_rel"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background_login"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="cn.hsd.student.activity.login.InfoActivity">

    <TextView
        android:id="@+id/wqj_info"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="请填写实习登记表:"
        android:textColor="#000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/info_tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/wqj_info"
        android:layout_marginTop="10dip"
        android:text="实习名称:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/wqj_info"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="XX公司"
        android:inputType="text" />

    <TextView
        android:id="@+id/info_tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv1"
        android:layout_marginTop="10dip"
        android:text="实习专业:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv1"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="软件工程"
        android:inputType="text" />

    <TextView
        android:id="@+id/info_tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv2"
        android:layout_marginTop="10dip"
        android:text="开始时间:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv2"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="2016.07.15"
        android:inputType="datetime" />

    <TextView
        android:id="@+id/info_tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv3"
        android:layout_marginTop="10dip"
        android:inputType="datetime"
        android:text="结束时间:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_et3"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="2017.04.30"
        android:inputType="datetime"/>

    <TextView
        android:id="@+id/info_tv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv4"
        android:layout_marginTop="10dip"
        android:text="指导老师:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et5"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_et4"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="XXX"
        android:inputType="text" />

    <TextView
        android:id="@+id/info_tv6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv5"
        android:layout_marginTop="10dip"
        android:text="联系电话:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et6"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_et5"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="186-xxxx-xxxx"
        android:inputType="phone" />

    <TextView
        android:id="@+id/info_tv7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv6"
        android:layout_marginTop="10dip"
        android:text="QQ  号码:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et7"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_et6"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="xxxxxxxx"
        android:inputType="number" />

    <TextView
        android:id="@+id/info_tv8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv7"
        android:layout_marginTop="10dip"
        android:text="微信号码:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et8"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_et7"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="xxxxxxxx"
        android:inputType="number" />

    <TextView
        android:id="@+id/info_tv9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv8"
        android:layout_marginLeft="2dip"
        android:layout_marginTop="10dip"
        android:text="年       龄:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et9"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_et8"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="21"
        android:inputType="number" />

    <TextView
        android:id="@+id/info_tv10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv9"
        android:layout_marginTop="10dip"
        android:text="身份证号:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et10"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_et9"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="xxxxxxxx"
        android:inputType="number" />

    <TextView
        android:id="@+id/info_tv11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_tv10"
        android:layout_marginTop="10dip"
        android:text="家庭住址:"
        android:textColor="#000"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/info_et11"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_et10"
        android:layout_marginLeft="100dip"
        android:layout_marginTop="10dip"
        android:background="@drawable/rounded_edittext"
        android:hint="xxxxxxxx"
        android:inputType="text" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/info_et11"
        android:layout_marginTop="10dip"
        android:background="@drawable/blue_button"
        android:onClick="commit"
        android:text="提交"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="20dp"
        />


</RelativeLayout>
